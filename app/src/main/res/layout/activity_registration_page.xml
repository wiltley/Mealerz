<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/registerconstraintlayout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#ee5d6c"
    tools:context=".RegistrationPage">

    <TextView
        android:id="@+id/register"
        android:layout_width="360dp"
        android:layout_height="87dp"
        android:layout_marginStart="19dp"
        android:layout_marginTop="22dp"
        android:layout_marginEnd="32dp"
        android:layout_marginBottom="622dp"
        android:text="REGISTER"
        android:textColor="#eeaf61"
        android:textSize="34sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/email2"
        android:layout_width="147dp"
        android:layout_height="47dp"
        android:layout_marginStart="21dp"
        android:layout_marginTop="48dp"
        android:layout_marginEnd="332dp"
        android:layout_marginBottom="516dp"
        android:text="Email"
        android:textColor="#eeaf61"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.022"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/login"
        app:layout_constraintVertical_bias="0.032" />

    <com.google.android.material.textfield.TextInputEditText
        android:id="@+id/emailinput2"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginBottom="462dp"
        android:hint="(Please Enter Your Email Here)"
        android:minHeight="48dp"
        android:textColor="#eeaf61"
        android:textColorHint="#eeaf61"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/email"
        app:layout_constraintVertical_bias="1.0" />

    <com.google.android.material.textfield.TextInputLayout
        android:id="@+id/emaillayout"
        android:layout_width="409dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="1dp"
        android:layout_marginTop="1dp"
        android:layout_marginEnd="1dp"
        android:layout_marginBottom="460dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/emailinput">

    </com.google.android.material.textfield.TextInputLayout>

    <TextView
        android:id="@+id/password2"
        android:layout_width="146dp"
        android:layout_height="48dp"
        android:layout_marginStart="20dp"
        android:layout_marginTop="102dp"
        android:layout_marginEnd="245dp"
        android:text="Password"
        android:textColor="#eeaf61"
        android:textSize="20sp"
        app:layout_constraintBottom_toTopOf="@+id/passwordinput2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/emailinput2" />

    <com.google.android.material.textfield.TextInputEditText
        android:id="@+id/passwordinput2"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginBottom="264dp"
        android:hint="(Please Enter Your Password Here)"
        android:minHeight="48dp"
        android:textColor="#eeaf61"
        android:textColorHint="#eeaf61"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/email"
        app:layout_constraintVertical_bias="1.0" />

    <Button
        android:id="@+id/regiesterbutton2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="153dp"
        android:layout_marginTop="42dp"
        android:layout_marginEnd="164dp"
        android:layout_marginBottom="148dp"
        android:text="REGISTER"
        android:textSize="24sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.676"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/passwordinput"
        app:layout_constraintVertical_bias="0.159" />

    <TextView
        android:id="@+id/haveanaccount3"
        android:layout_width="225dp"
        android:layout_height="52dp"
        android:layout_marginStart="23dp"
        android:layout_marginTop="55dp"
        android:layout_marginEnd="232dp"
        android:layout_marginBottom="68dp"
        android:text="Already Have An Account?"
        android:textColor="#eeaf61"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/loginbutton"
        app:layout_constraintVertical_bias="0.25" />

    <Button
        android:id="@+id/loginbutton2"
        android:layout_width="137dp"
        android:layout_height="56dp"
        android:layout_marginStart="14dp"
        android:layout_marginTop="48dp"
        android:layout_marginEnd="16dp"
        android:layout_marginBottom="68dp"
        android:text="LOGIN"
        android:textSize="16sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.122"
        app:layout_constraintStart_toEndOf="@+id/haveanaccount"
        app:layout_constraintTop_toBottomOf="@+id/loginbutton"
        app:layout_constraintVertical_bias="0.75" />

</androidx.constraintlayout.widget.ConstraintLayout>